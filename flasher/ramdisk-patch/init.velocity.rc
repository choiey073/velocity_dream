on boot
	# Base settings

	# UKSM
	write /sys/kernel/mm/uksm/cpu_governor medium
	write /sys/kernel/mm/uksm/sleep_millisecs 150
	write /sys/kernel/mm/uksm/max_cpu_percentage 90

	# ZRAM
	write /sys/block/zram0/reset 1
	write /sys/block/zram0/disksize 1048576000
	exec u:r:init:s0 root root -- mkswap /dev/block/zram0
	exec u:r:init:s0 root root -- swapon /dev/block/zram0

	# Virtual Memory
	write /proc/sys/vm/vfs_cache_pressure 80

	# Storage
	write /sys/block/sda/queue/iostats 0
	write /sys/block/sda/queue/read_ahead_kb 2048
	write /sys/block/mmcblk0/queue/iostats 0
	write /sys/block/mmcblk0/queue/read_ahead_kb 1024

on property:sys.boot_completed=1
	# Disable printk later to aid debugging during boot
	write /sys/kernel/printk_mode/printk_mode 0
